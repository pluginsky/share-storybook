{
  "name": "share-storybook",
  "version": "1.0.0",
  "description": "Place Storybook playground as embed on website",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "dist/register.js",
  "types": "dist/register.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "npm run build -- --watch",
    "storybook": "start-storybook -p 6006",
    "prettier": "prettier --write 'src/**/*' 'test/**/*' 'README.md' '/docs/**/*.md'",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pluginsky/share-storybook.git"
  },
  "keywords": [
    "share",
    "addon",
    "embed",
    "storybook",
    "place"
  ],
  "files": [
    "dist/**/*",
    "register.js"
  ],
  "homepage": "",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addons": "^6.3.6",
    "@storybook/api": "^6.3.6",
    "@storybook/components": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@types/node": "^16.4.12",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-json-tree": "^0.6.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.2.2",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run prettier",
      "npm run lint"
    ]
  }
}
